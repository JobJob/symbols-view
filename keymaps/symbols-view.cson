'.platform-darwin atom-text-editor':
  'cmd-r': 'symbols-view-click:toggle-file-symbols'
  'cmd-alt-down': 'symbols-view-click:go-to-declaration'
  'cmd-alt-up': 'symbols-view-click:return-from-declaration'

'.platform-win32 atom-text-editor':
  'ctrl-r': 'symbols-view-click:toggle-file-symbols'

'.platform-linux atom-text-editor':
  'ctrl-r': 'symbols-view-click:toggle-file-symbols'
  'ctrl-alt-down': 'symbols-view-click:go-to-declaration'
  'ctrl-alt-up': 'symbols-view-click:return-from-declaration'

'.platform-darwin':
  'cmd-shift-r': 'symbols-view-click:toggle-project-symbols'

'.platform-win32, .platform-linux':
  'ctrl-shift-r': 'symbols-view-click:toggle-project-symbols'
